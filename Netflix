#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    int a,b,c;
	    cin>>a>>b>>c;
	    int xrupee;
	    cin>>xrupee;
	    if((a+b)>=xrupee || (b+c) >= xrupee || (c+a)>=xrupee)
	    {
	        cout << "YES" <<endl;
	    }
	    else cout << "NO" << endl;
	}

}
