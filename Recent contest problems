import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		for (int i=0; i<t; i++){
		    int startcount = 0;
		    int ltimecount = 0;
		    int t1 = sc.nextInt();
		    
		    for(int j=0;j<t1; j++){
		        String S = sc.next();
		        if(S.equals("START38")){
		            startcount++;
		        }
		        else{
		            ltimecount++;
		        }
		    }
		    System.out.println(startcount + " " + ltimecount);
		}

	}
}
