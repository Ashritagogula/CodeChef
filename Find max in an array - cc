import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
    public static void main (String[] args) throws java.lang.Exception
    {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt(); // Number of test cases

        for(int j = 0; j < t; j++){
            int n = sc.nextInt(); // Number of mountains for this test case
            long max = 0; // Use long to store heights
            for(int i = 0; i < n; i++){
                long height = sc.nextLong();
                if(height > max){
                    max = height;
                }
            }
            System.out.println(max);
        }
    }
}
