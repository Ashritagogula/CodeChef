#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0;i<t;i++){
	    int D,L,R;
	    cin>>D>>L>>R;
	    if(D>=L && D<=R){
	        std::cout << "Take second dose now" << std::endl;
	    }
	    else if(D>R){
	        std::cout << "Too Late" << std::endl ;
	    }
	    else if(D<L){
	        std::cout << "Too Early" << std::endl;
	    }
	}

}
