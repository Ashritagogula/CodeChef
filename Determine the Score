#include <stdio.h>

int main(void) {
	// your code goes here
	int T;
	scanf("%d",&T);
	for(int i=0;i<T;i++)
	{
	    int x;
	    scanf("%d",&x);  //points //10 testcases --> same points for each test case
	   int N; //number of test cases among 10;
	   scanf(" %d",&N);
	   printf("%d\n",x/10*N);
	   
	    
	}

}

